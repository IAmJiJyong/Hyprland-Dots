  "battery": {
    "interval": 60,
    "states": {
      "warning": 30,
      "critical": 15,
    },
    "max-length": 10,
    "format": "{icon} {capacity}%",
    "format-warning": "{icon} {capacity}%",
    "format-critical": "{icon} {capacity}%",
    "format-charging": "ÔñÉ {capacity}%",
    "format-plugged": "Ôá¶ {capacity}%",
    "format-alt": "{icon} {capacity}%",
    "format-full": "Ôá¶ 100%",
    "format-icons": [
      "Ôïπ",
      "Ôï∫",
      "Ôïª",
      "Ôïª",
      "ÔïΩ",
      "Ôïæ",
      "Ôïø",
      "ÔñÄ",
      "ÔñÅ",
      "Ôï∏"
    ],
  },
