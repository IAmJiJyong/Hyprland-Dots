{
  "layer": "top",
  "position": "top",
  "height": 60,
  "margin-left": 50,
  "margin-right": 50,
  "modules-left": [
    "clock",
    "custom/wttr",
    "hyprland/window"
  ],
  "modules-center": [
    "hyprland/workspaces"
  ],
  "modules-right": [
    "group/efficacy",
    "group/hardware",
    "group/system",
    "custom/wallpaper",
  ],
  "group/efficacy": {
    "orientation": "horizontal",
    "modules": [
      "cpu",
      "memory",
      "disk"
    ],
  },
  "group/hardware": {
    "orientation": "horizontal",
    "modules": [
      "pulseaudio",
      "backlight",
      "battery"
    ],
  },
  "group/system": {
    "orientation": "horizontal",
    "modules": [
      "network",
      "tray",
      "custom/swaync"
    ],
  },
  "custom/wallpaper": {
    "format": "Ôåí ",
    "tooltip": false,
    "on-click": "~/.config/hypr/scripts/WallpaperRandom.sh ~/Pictures/wallpapers",
    "on-click-right": "~/.config/hypr/scripts/WallpaperSelect.sh ~/Pictures/wallpapers",
  },
  "mpris": {
    "interval": 1,
    "format": "{player_icon} {dynamic}",
    "format-paused": "{status_icon} <i>{dynamic}</i>",
    "title-len": 20,
    "artist-len": 20,
    "ignored-players": [
      "firefox"
    ],
    "player-icons": {
      "plasma-browser-integration": "Ôâ© ",
      "spotify": "Û∞ìá",
      "default": "‚ñ∂",
      "mpv": "üéµ",
    },
    "status-icons": {
      "paused": "‚è∏Ô∏è",
    },
  },
  "clock": {
    "format": "{:%H:%M} ÔÄó ",
    "format-alt": "{:%A, %B %d, %Y (%R)} ÔóØ ",
    "tooltip-format": "<tt><small>{calendar}</small></tt>",
    "calendar": {
      "mode": "year",
      "mode-mon-col": 3,
      "weeks-pos": "right",
      "on-scroll": 1,
      "format": {
        "months": "<span color='#ffead3'><b>{}</b></span>",
        "days": "<span color='#ecc6d9'><b>{}</b></span>",
        "weeks": "<span color='#99ffdd'><b>W{}</b></span>",
        "weekdays": "<span color='#ffcc66'><b>{}</b></span>",
        "today": "<span color='#ff6699'><b><u>{}</u></b></span>",
      },
    },
    "actions": {
      "on-click-right": "mode",
      "on-click-forward": "tz_up",
      "on-click-backward": "tz_down",
      "on-scroll-up": "shift_up",
      "on-scroll-down": "shift_down",
    },
  },
  "custom/wttr": {
    "format": "{}",
    "tooltip": true,
    "interval": 3600,
    "exec": "~/.config/hypr/scripts/wttr.py",
    "return-type": "json",
  },
  "cpu": {
    "interval": 1,
    "format": "Ôíº {usage}%",
  },
  "memory": {
    "interval": 1,
    "format": "Ô¨ô {percentage}%",
    "states": {
      "warning": 85,
    },
  },
  "disk": {
    "interval": 30,
    "format": "ÔÇ† {used}",
    "path": "/home/iamjijyong",
    "tooltip": true,
    "tooltip-format": "{used}/{total} => {path} {percentage_used}%",
  },
  "wireplumber": {
    "format": "{icon} {volume}%",
    "format-muted": "ÔÄ¶",
    "on-click": "helvum",
    "max-volume": 150,
    "format-icons": {
      "headphone": "ÔÄ•",
      "hands-free": "Ôñê",
      "headset": "Û∞ãé",
      "phone": "ÔÇï",
      "portable": "ÔÇï",
      "car": "ÔÜπ",
      "default": [
        "ÔÄ¶",
        "ÔÄß",
        "ÔÄ®"
      ],
    },
    "scroll-step": 0.2,
  },
  "pulseaudio": {
    "format": "{icon} {volume}%",
